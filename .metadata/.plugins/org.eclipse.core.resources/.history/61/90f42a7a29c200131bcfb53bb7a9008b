package jxau.sms.li.tecBasicInfo.po;

import java.util.Date;

public class TecBasicInfo {
	private String eduBackground;

	private String exameState;

	private String hometown;

	private String nation;

	private String political;

	private String remarks;

	private int sex;

	private String teacherName;

	private String teacherNo;//教工号

	private String teacherTitle;

	private String tecState;

	private Date titleTime;

	private Date worktime;//入校时间

	public String getEduBackground() {
		return eduBackground;
	}

	public String getExameState() {
		return exameState;
	}

	public String getHometown() {
		return hometown;
	}

	public String getNation() {
		return nation;
	}

	public String getPolitical() {
		return political;
	}

	public String getRemarks() {
		return remarks;
	}

	public int getSex() {
		return sex;
	}

	public String getTeacherName() {
		return teacherName;
	}

	public String getTeacherNo() {
		return teacherNo;
	}

	public String getTeacherTitle() {
		return teacherTitle;
	}

	public String getTecState() {
		return tecState;
	}

	public Date getTitleTime() {
		return titleTime;
	}

	public Date getWorktime() {
		return worktime;
	}

	public void setEduBackground(String eduBackground) {
		this.eduBackground = eduBackground;
	}
	public void setExameState(String exameState) {
		this.exameState = exameState;
	}
	public void setHometown(String hometown) {
		this.hometown = hometown;
	}
	public void setNation(String nation) {
		this.nation = nation;
	}
	public void setPolitical(String political) {
		this.political = political;
	}
	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}
	public void setSex(int sex) {
		this.sex = sex;
	}
	public void setTeacherName(String teacherName) {
		this.teacherName = teacherName;
	}
	public void setTeacherNo(String teacherNo) {
		this.teacherNo = teacherNo;
	}
	public void setTeacherTitle(String teacherTitle) {
		this.teacherTitle = teacherTitle;
	}
	public void setTecState(String tecState) {
		this.tecState = tecState;
	}
	public void setTitleTime(Date titleTime) {
		this.titleTime = titleTime;
	}
	public void setWorktime(Date worktime) {
		this.worktime = worktime;
	}
	
	public String toString() {
		return "teacherNo:"+teacherNo+",eduBackground:"+eduBackground
				+",teacherName:"+teacherName+",nation:"+nation
				+",political:"+political+",remarks:"+remarks+",sex:"+sex+",teacherTitle:"+teacherTitle
				+",titleTime:"+titleTime+",tecState:"+tecState+",worktime"+worktime;
	}
	
	
}
