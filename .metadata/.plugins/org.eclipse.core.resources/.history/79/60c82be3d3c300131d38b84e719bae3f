<mapper namespace="jxau.sms.qing.login.dao.LoginDao">  
  <select id="selectStudent"  parameterType= "HashMap"  resultType="int">
  		select count(1) from stu_info where studentNo = #{studentNo}
  		 and stuPassword = #{stuPassword};
  </select>
  
   <select id="selectTeacher"  parameterType="int" resultType="int">
  		select * from tec_info where teacherNo = #{teacherNo} ;
  </select> 
   
 </mapper>